<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>indexjson.jsp</welcome-file>
    </welcome-file-list>
    
    
    <servlet>
        <servlet-name>provinceServlet</servlet-name>
        <servlet-class>com.colin.controller.ProvinceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>provinceServlet</servlet-name>
        <url-pattern>/searchProvince</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>provinceJsonServlet</servlet-name>
        <servlet-class>com.colin.controller.ProvinceJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>provinceJsonServlet</servlet-name>
        <url-pattern>/searchJsonProvince</url-pattern>
    </servlet-mapping>
</web-app>